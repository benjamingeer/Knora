webern:hasFamilyName  rdf:type  owl:ObjectProperty;
        rdfs:comment        "Repräsentiert den Nachnamen einer Person."@de , "Represents the family name of a person."@en , "Représente le nom de famille d'une personne."@fr , "Rappresenta il nome di famiglia di una persona."@it;
        rdfs:label          "Nachname"@de , "nom de famille"@fr , "nome di famiglia"@it , "family name"@en;
        rdfs:subPropertyOf  knora-base:hasValue;
        knora-base:objectClassConstraint
                knora-base:UnformattedTextValue;
        knora-base:subjectClassConstraint
                webern:Person .

webern:Person  rdf:type          owl:Class;
        rdfs:comment             "A resource representing a person"@en;
        rdfs:label               "Person"@de , "Personne"@fr , "Persona"@it , "Person"@en;
        rdfs:subClassOf          foaf:Person , knora-base:Resource;
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "8"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "0"^^xsd:nonNegativeInteger;
                                   owl:onProperty       webern:hasBiography
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "1"^^xsd:nonNegativeInteger;
                                   owl:maxCardinality   "1"^^xsd:nonNegativeInteger;
                                   owl:onProperty       webern:hasGivenName
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "7"^^xsd:nonNegativeInteger;
                                   owl:maxCardinality   "1"^^xsd:nonNegativeInteger;
                                   owl:onProperty       webern:hasDeathPlace
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "10"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "0"^^xsd:nonNegativeInteger;
                                   owl:onProperty       webern:hasRelation
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "9"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "0"^^xsd:nonNegativeInteger;
