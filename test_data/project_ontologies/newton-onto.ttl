<http://www.knora.org/ontology/0801/newton>
        rdf:type                      owl:Ontology;
        rdfs:label                    "The Newton-Project ontology";
        knora-base:attachedToProject  <http://rdfh.ch/projects/yTerZGyxjZVqFMNNKXCDPF>;
        knora-base:lastModificationDate
                "2012-12-12T12:12:12.12Z"^^xsd:dateTime .

<http://www.knora.org/ontology/0801/newton#hasAbbreviatedTitle>
        rdf:type            owl:ObjectProperty;
        rdfs:comment        "Represents the abbreviated title of the letter."@en;
        rdfs:label          "Abk√ºrzung des Titels"@de , "Abbreviation of Title"@en;
        rdfs:subPropertyOf  knora-base:hasValue;
        knora-base:objectClassConstraint
                knora-base:UnformattedTextValue;
        knora-base:subjectClassConstraint
                <http://www.knora.org/ontology/0801/newton#letter> .

<http://www.knora.org/ontology/0801/newton#letter>
        rdf:type                 owl:Class;
        rdfs:comment             "A resource representing a letter from The Newton Project"@en;
        rdfs:label               "Brief"@de , "Lettre"@fr , "Letter"@en;
        rdfs:subClassOf          beol:basicLetter;
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "0"^^xsd:nonNegativeInteger;
                                   owl:maxCardinality   "1"^^xsd:nonNegativeInteger;
                                   owl:onProperty       <http://www.knora.org/ontology/0801/newton#hasAbbreviatedTitle>
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "3"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "0"^^xsd:nonNegativeInteger;
                                   owl:onProperty       <http://www.knora.org/ontology/0801/newton#isReplyToValue>
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "3"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "0"^^xsd:nonNegativeInteger;
                                   owl:onProperty       <http://www.knora.org/ontology/0801/newton#isReplyTo>
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "1"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "1"^^xsd:nonNegativeInteger;
                                   owl:onProperty       <http://www.knora.org/ontology/0801/newton#newtonProjectID>
                                 ];
        rdfs:subClassOf          [ rdf:type             owl:Restriction;
                                   salsah-gui:guiOrder  "2"^^xsd:nonNegativeInteger;
                                   owl:minCardinality   "0"^^xsd:nonNegativeInteger;
                                   owl:onProperty       <http://www.knora.org/ontology/0801/newton#hasFacsimiles>
                                 ];
        knora-base:resourceIcon  "letter.png" .

<http://www.knora.org/ontology/0801/newton#newtonProjectID>
        rdf:type            owl:ObjectProperty;
        rdfs:comment        "Represents the letter in The Newton Project."@en;
        rdfs:label          "Letter in The Newton Project"@de , "Letter in The Newton Project"@en;
        rdfs:subPropertyOf  knora-base:hasValue;
        knora-base:objectClassConstraint
                knora-base:UnformattedTextValue;
        knora-base:subjectClassConstraint
                <http://www.knora.org/ontology/0801/newton#letter> .

<http://www.knora.org/ontology/0801/newton#hasFacsimiles>
        rdf:type            owl:ObjectProperty;
        rdfs:comment        "Die Faksimilen von deisem Brief ist vorhanden in"@de , "The Facsimiles of this letter are available in"@en;
        rdfs:label          "Faksimile"@de , "Facsimile"@en;
        rdfs:subPropertyOf  knora-base:hasValue;
        knora-base:objectClassConstraint
                knora-base:UriValue;
        knora-base:subjectClassConstraint
                <http://www.knora.org/ontology/0801/newton#letter> .

<http://www.knora.org/ontology/0801/newton#isReplyTo>
        rdf:type               owl:ObjectProperty;
        rdfs:comment           "Der Brief ist eine Antwort auf einen Brief"@de , "Letter is reply to another letter"@en;
        rdfs:label             "Antwort auf"@de , "Reply to"@en;
        rdfs:subPropertyOf     knora-base:hasLinkTo;
        knora-base:objectClassConstraint
                <http://www.knora.org/ontology/0801/newton#letter>;
        salsah-gui:guiElement  salsah-gui:Searchbox .

<http://www.knora.org/ontology/0801/newton#isReplyToValue>
        rdf:type            owl:ObjectProperty;
        rdfs:comment        "Der Brief ist eine Antwort auf einen Brief"@de , "Letter is reply to another letter"@en;
        rdfs:label          "Antwort auf"@de , "Reply to"@en;
        rdfs:subPropertyOf  knora-base:hasLinkToValue;
        knora-base:objectClassConstraint
                knora-base:LinkValue;
        knora-base:subjectClassConstraint
                <http://www.knora.org/ontology/0801/newton#letter> .
