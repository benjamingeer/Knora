<http://www.knora.org/ontology/knora-base> {

  <http://www.knora.org/ontology/knora-base>
    rdf:type                     owl:Ontology ;
    rdfs:label                   "The Knora base ontology"@en ;
    knora-base:attachedToProject knora-admin:SystemProject ;
    knora-base:ontologyVersion   "knora-base v25" .
}

<http://www.knora.org/ontology/0001/freetest> {

  <http://www.knora.org/ontology/0001/freetest>
      rdf:type                        owl:Ontology ;
      rdfs:label                      "The free test ontology" ;
      knora-base:attachedToProject    <http://rdfh.ch/projects/0001> ;
      knora-base:lastModificationDate "2012-12-12T12:12:12.12Z"^^xsd:dateTime .

  freetest:hasSuperText
      rdf:type                          owl:ObjectProperty ;
      rdfs:subPropertyOf                knora-base:hasValue ;
      knora-base:subjectClassConstraint freetest:FreeTest ;
      knora-base:objectClassConstraint  knora-base:TextValue ;
      salsah-gui:guiElement             salsah-gui:SimpleText ;
      salsah-gui:guiAttribute           "size=80",
                                        "maxlength=255" .

  freetest:hasUnformattedSubText
      rdf:type                          owl:ObjectProperty ;
      rdfs:subPropertyOf                freetest:hasSuperText ;
      rdfs:label                        "Text Area Text"@en ;
      knora-base:subjectClassConstraint freetest:FreeTest ;
      knora-base:objectClassConstraint  knora-base:TextValue ;
      salsah-gui:guiElement             salsah-gui:SimpleText ;
      salsah-gui:guiAttribute           "size=80",
                                        "maxlength=255" .

  freetest:hasFormattedSubText
      rdf:type                          owl:ObjectProperty ;
      rdfs:subPropertyOf                freetest:hasSuperText ;
      knora-base:subjectClassConstraint freetest:FreeTest ;
      knora-base:objectClassConstraint  knora-base:TextValue ;
      salsah-gui:guiElement             salsah-gui:Richtext .
}

