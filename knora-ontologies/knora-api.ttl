@prefix knora-base: <http://www.knora.org/ontology/knora-base#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix : <http://www.knora.org/ontology/knora-api#> .

<http://www.knora.org/ontology/knora-api> rdf:type owl:Ontology .

### http://www.knora.org/ontology/knora-api#Resource

:Resource rdf:type owl:Class .


### http://www.knora.org/ontology/knora-api#Value

:Value rdf:type owl:Class ;

    rdfs:subClassOf [ rdf:type owl:Restriction ;
                      owl:onProperty :valueAsString ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                    ] .


### http://www.knora.org/ontology/knora-api#resourceProperty

:resourceProperty rdf:type owl:ObjectProperty .


### http://www.knora.org/ontology/knora-api#hasValue

:hasValue rdf:type owl:ObjectProperty ;

     rdfs:subPropertyOf :resourceProperty ;

     knora-base:subjectClassConstraint :Resource ;

     knora-base:objectClassConstraint :Value .


### http://www.knora.org/ontology/knora-api#hasLinkTo

:hasLinkTo rdf:type owl:ObjectProperty ;

      rdfs:subPropertyOf :resourceProperty ;

      rdfs:label "hat Link zu"@de ,
                 "has Link to"@en ,
                 "a lien vers"@fr ,
                 "ha Link verso"@it ;

      rdfs:comment "Represents a direct connection between two resources"@en ;

      knora-base:objectClassConstraint :Resource ;

      knora-base:subjectClassConstraint :Resource .


### http://www.knora.org/ontology/knora-api#valueAsString

:valueAsString rdf:type owl:DatatypeProperty ;

    rdfs:comment "A plain string representation of a value"@en ;

    knora-base:subjectClassConstraint :Value ;

    knora-base:objectDatatypeConstraint xsd:string .


### Text value

###  http://www.knora.org/ontology/knora-base#TextValue

:TextValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textValueAsXml ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textValueHasMapping ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textValueAsHtml ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://www.knora.org/ontology/knora-api#textValueAsXml

:textValueAsXml rdf:type owl:DatatypeProperty ;

    rdfs:comment "A text value represented in XML."@en ;

    knora-base:subjectClassConstraint :TextValue ;

    knora-base:objectDatatypeConstraint xsd:string .


### http://www.knora.org/ontology/knora-api#textValueHasMapping

:textValueHasMapping rdf:type owl:ObjectProperty ;

    rdfs:comment "The mapping used to turn standoff into XML and vice versa."@en ;

    knora-base:subjectClassConstraint :TextValue ;

    knora-base:objectClassConstraint knora-base:XMLToStandoffMapping .


### http://www.knora.org/ontology/knora-api#textValueAsHtml

:textValueAsHtml rdf:type owl:DatatypeProperty ;

    rdfs:comment "A text value represented in HTML."@en ;

    knora-base:subjectClassConstraint :TextValue ;

    knora-base:objectClassConstraint knora-base:XMLToStandoffMapping .

### Date value

###  http://www.knora.org/ontology/knora-base#DateValue

:DateValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasStartYear ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasEndYear ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasStartMonth ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasEndMonth ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasStartDay ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasEndDay ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasCalendar ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://www.knora.org/ontology/knora-api#dateValueHasStartYear

:dateValueHasStartYear rdf:type owl:DatatypeProperty ;

    rdfs:comment "The opening year of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### http://www.knora.org/ontology/knora-api#dateValueHasEndYear

:dateValueHasEndYear rdf:type owl:DatatypeProperty ;

    rdfs:comment "The closing year of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### http://www.knora.org/ontology/knora-api#dateValueHasStartMonth

:dateValueHasStartMonth rdf:type owl:DatatypeProperty ;

    rdfs:comment "The opening month of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### http://www.knora.org/ontology/knora-api#dateValueHasEndMonth

:dateValueHasEndMonth rdf:type owl:DatatypeProperty ;

    rdfs:comment "The closing month of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### http://www.knora.org/ontology/knora-api#dateValueHasStartDay

:dateValueHasStartDay rdf:type owl:DatatypeProperty ;

    rdfs:comment "The opening day of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### http://www.knora.org/ontology/knora-api#dateValueHasEndDay

:dateValueHasEndDay rdf:type owl:DatatypeProperty ;

    rdfs:comment "The clsong day of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### http://www.knora.org/ontology/knora-api#dateValueHasCalendar

:dateValueHasCalendar rdf:type owl:DatatypeProperty ;

    rdfs:comment "The calendar of a date value"@en ;

    knora-base:subjectClassConstraint :DateValue ;

    knora-base:objectDatatypeConstraint xsd:string .


### Link value

###  http://www.knora.org/ontology/knora-base#LinkValue

:LinkValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :linkValueHasTarget ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://www.knora.org/ontology/knora-api#linkValueHasTarget

:linkValueHasTarget rdf:type owl:ObjectProperty ;

    rdfs:comment "The target of the link property (the referred resource)"@en ;

    knora-base:subjectClassConstraint :LinkValue ;

    knora-base:objectClassConstraint :Resource .


### Integer Value

###  http://www.knora.org/ontology/knora-base#IntegerValue

:IntegerValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :integerValueAsInteger ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://www.knora.org/ontology/knora-api#integerValueAsInteger

:integerValueAsInteger rdf:type owl:ObjectProperty ;

    rdfs:comment "The integer value as an integer"@en ;

    knora-base:subjectClassConstraint :IntegerValue ;

    knora-base:objectDatatypeConstraint xsd:integer .


### Decimal Value

###  http://www.knora.org/ontology/knora-base#IntegerValue

:DecimalValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :decimalValueAsDecimal ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://www.knora.org/ontology/knora-api#decimalValueAsDecimal

:decimalValueAsDecimal rdf:type owl:ObjectProperty ;

    rdfs:comment "The decimal value as a decimal"@en ;

    knora-base:subjectClassConstraint :DecimalValue ;

    knora-base:objectDatatypeConstraint xsd:decimal .

### FileValue

:FileValue rdfs:subClassOf :Value,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :fileValueIsPreview ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :fileValueAsUrl ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://www.knora.org/ontology/knora-api#fileValueIsPreview

:fileValueIsPreview rdf:type owl:ObjectProperty ;

    rdfs:comment "Indicates if this is file value is a preview"@en ;

    knora-base:subjectClassConstraint :FileValue ;

    knora-base:objectDatatypeConstraint xsd:boolean .


### http://www.knora.org/ontology/knora-api#fileValueAsUrl

:fileValueAsUrl rdf:type owl:ObjectProperty ;

    rdfs:comment "The path to retrieve the file value"@en ;

    knora-base:subjectClassConstraint :FileValue ;

    knora-base:objectDatatypeConstraint xsd:anyURI .


### StillImageFileValue

:StillImageFileValue rdfs:subClassOf :FileValue .


