@prefix knora-base: <http://www.knora.org/ontology/knora-base#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix : <http://api.knora.org/ontology/knora-api/v2#> .

<http://api.knora.org/ontology/knora-api/v2> rdf:type owl:Ontology .

### http://api.knora.org/ontology/knora-api/v2#hasOntologies

:hasOntologies rdf:type owl:ObjectProperty ;

    :objectClassConstraint :OntologyList ;

    rdfs:label "has ontologies"@en ;

    rdfs:comment "Points to a list of ontologies available on this Knora instance."@en .


### http://api.knora.org/ontology/knora-api/v2#OntologyList

:OntologyList rdf:type rdf:List ;

    rdfs:label "A list of ontology Iris"@en ;

    rdfs:comment "A list of ontologies available on this Knora instance."@en .


### http://api.knora.org/ontology/knora-api/v2#hasOntologiesWithResourceClasses

:hasOntologiesWithResourceClasses rdf:type owl:ObjectProperty ;

    rdfs:label "A map of ontology Iris to a list of resource classes"@en ;

    rdfs:comment "A map of ontology Iris to a list of resource classes."@en .


### http://api.knora.org/ontology/knora-api/v2#hasProperties

:hasProperties rdf:type owl:ObjectProperty ;

    rdfs:label "A map of property Iris to property definitions."@en ;

    rdfs:comment "A map of property Iris to property definitions."@en .


### http://api.knora.org/ontology/knora-api/v2#belongsToOntology

:belongsToOntology rdf:type owl:DatatypeProperty ;

    rdfs:label "The ontology a entity definition belongs to."@en ;

    rdfs:comment "The ontology a entity definition belongs to."@en .


### http://api.knora.org/ontology/knora-api/v2#hasResourceClasses

:hasResourceClasses rdf:type owl:ObjectProperty ;

    rdfs:label "A map of resource class Iris to resource class definitions."@en ;

    rdfs:comment "A map of resource class Iris to resource class definitions."@en .


### http://api.knora.org/ontology/knora-api/v2#Resource

:Resource rdf:type owl:Class .


### http://api.knora.org/ontology/knora-api/v2#Value

:Value rdf:type owl:Class ;

    rdfs:subClassOf [ rdf:type owl:Restriction ;
                      owl:onProperty :valueAsString ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                    ] .


### http://api.knora.org/ontology/knora-api/v2#resourceProperty

:resourceProperty rdf:type owl:ObjectProperty .


### http://api.knora.org/ontology/knora-api/v2#hasValue

:hasValue rdf:type owl:ObjectProperty ;

     rdfs:subPropertyOf :resourceProperty ;

     :subjectClassConstraint :Resource ;

     :objectClassConstraint :Value .


### http://api.knora.org/ontology/knora-api/v2#hasLinkTo

:hasLinkTo rdf:type owl:ObjectProperty ;

      rdfs:subPropertyOf :resourceProperty ;

      rdfs:label "hat Link zu"@de ,
                 "has Link to"@en ,
                 "a lien vers"@fr ,
                 "ha Link verso"@it ;

      rdfs:comment "Represents a direct connection between two resources"@en ;

      :objectClassConstraint :Resource ;

      :subjectClassConstraint :Resource .


###  http://api.knora.org/ontology/knora-api/v2#valueHas

:valueHas rdf:type rdf:Property ;

          :subjectClassConstraint :Value .


###  http://api.knora.org/ontology/knora-api/v2#subjectClassConstraint

:subjectClassConstraint rdf:type owl:ObjectProperty ;

                   rdfs:comment "Specifies the OWL class that the subject of a property must belong to"@en .


###  http://api.knora.org/ontology/knora-api/v2#objectClassConstraint

:objectClassConstraint rdf:type owl:ObjectProperty ;

                  rdfs:comment "Specifies the OWL class that the object of a property must belong to"@en .


###  http://api.knora.org/ontology/knora-api/v2#objectDatatypeConstraint

:objectDatatypeConstraint rdf:type owl:DatatypeProperty ;

                          rdfs:comment "Specifies the type required as the object of a datatype property"@en .


###  http://api.knora.org/ontology/knora-api/v2#resourceIcon

:resourceIcon rdf:type owl:DatatypeProperty ;

              :subjectClassConstraint owl:Class ;

              :objectDatatypeConstraint xsd:string .


### http://api.knora.org/ontology/knora-api/v2#valueAsString

:valueAsString rdf:type owl:DatatypeProperty ;

    rdfs:comment "A plain string representation of a value"@en ;

    :subjectClassConstraint :Value ;

    :objectDatatypeConstraint xsd:string .


### Text value

###  http://api.knora.org/ontology/knora-api/v2#TextValue

:TextValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textValueAsXml ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textValueHasMapping ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :textValueAsHtml ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://api.knora.org/ontology/knora-api/v2#textValueAsXml

:textValueAsXml rdf:type owl:DatatypeProperty ;

    rdfs:comment "A text value represented in XML."@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :TextValue ;

    :objectDatatypeConstraint xsd:string .


### http://api.knora.org/ontology/knora-api/v2#textValueHasMapping

:textValueHasMapping rdf:type owl:ObjectProperty ;

    rdfs:comment "The mapping used to turn standoff into XML and vice versa."@en ;

    :subjectClassConstraint :TextValue ;

    :objectClassConstraint knora-base:XMLToStandoffMapping .


### http://api.knora.org/ontology/knora-api/v2#textValueAsHtml

:textValueAsHtml rdf:type owl:DatatypeProperty ;

    rdfs:comment "A text value represented in HTML."@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :TextValue ;

    :objectDatatypeConstraint xsd:string .

### Date value

###  http://api.knora.org/ontology/knora-api/v2#DateValue

:DateValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasStartYear ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasEndYear ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasStartMonth ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasEndMonth ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasStartDay ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasEndDay ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ],
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :dateValueHasCalendar ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://api.knora.org/ontology/knora-api/v2#dateValueHasStartYear

:dateValueHasStartYear rdf:type owl:DatatypeProperty ;

    rdfs:comment "The opening year of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:integer .


### http://api.knora.org/ontology/knora-api/v2#dateValueHasEndYear

:dateValueHasEndYear rdf:type owl:DatatypeProperty ;

    rdfs:comment "The closing year of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:integer .


### http://api.knora.org/ontology/knora-api/v2#dateValueHasStartMonth

:dateValueHasStartMonth rdf:type owl:DatatypeProperty ;

    rdfs:comment "The opening month of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:integer .


### http://api.knora.org/ontology/knora-api/v2#dateValueHasEndMonth

:dateValueHasEndMonth rdf:type owl:DatatypeProperty ;

    rdfs:comment "The closing month of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:integer .


### http://api.knora.org/ontology/knora-api/v2#dateValueHasStartDay

:dateValueHasStartDay rdf:type owl:DatatypeProperty ;

    rdfs:comment "The opening day of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:integer .


### http://api.knora.org/ontology/knora-api/v2#dateValueHasEndDay

:dateValueHasEndDay rdf:type owl:DatatypeProperty ;

    rdfs:comment "The clsong day of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:integer .


### http://api.knora.org/ontology/knora-api/v2#dateValueHasCalendar

:dateValueHasCalendar rdf:type owl:DatatypeProperty ;

    rdfs:comment "The calendar of a date value"@en ;

    rdfs:subPropertyOf :valueHas ;

    :subjectClassConstraint :DateValue ;

    :objectDatatypeConstraint xsd:string .


### Link value

###  http://api.knora.org/ontology/knora-api/v2#LinkValue

:LinkValue rdf:type owl:Class ;

           rdfs:subClassOf :Value ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :linkValueHasTarget ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :linkValueHasTargetIri ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .

### http://api.knora.org/ontology/knora-api/v2#linkValueHasTarget

:linkValueHasTarget rdf:type owl:ObjectProperty ;

    rdfs:comment "The target of the link property (the referred resource)"@en ;

    :subjectClassConstraint :LinkValue ;

    :objectClassConstraint :Resource .


### http://api.knora.org/ontology/knora-api/v2#linkValueHasTargetIri

:linkValueHasTargetIri rdf:type owl:DatatypeProperty ;

    rdfs:comment "The target IRI of the link property (the referred resource)"@en ;

    :subjectClassConstraint :LinkValue ;

    :objectDatatypeConstraint xsd:anyURI .


### Integer Value

###  http://api.knora.org/ontology/knora-api/v2#IntegerValue

:IntegerValue rdf:type owl:Class ;

