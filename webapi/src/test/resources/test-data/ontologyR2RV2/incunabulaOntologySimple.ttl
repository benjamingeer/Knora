@prefix knora-api: <http://api.knora.org/ontology/knora-api/simple/v2#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p0803-incunabula: <http://0.0.0.0:3333/ontology/0803/incunabula/simple/v2#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://0.0.0.0:3333/ontology/0803/incunabula/simple/v2> a owl:Ontology;
  rdfs:label "The incunabula ontology" .

p0803-incunabula:Sideband a owl:Class;
  rdfs:comment "Randleistentyp";
  rdfs:label "Randleiste";
  rdfs:subClassOf knora-api:StillImageRepresentation, [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty knora-api:creationDate
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasIncomingLink
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasStandoffLinkTo
    ], [ a owl:Restriction;
      owl:minCardinality 1;
      owl:onProperty knora-api:hasStillImageFile
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty knora-api:lastModificationDate
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty rdfs:label
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty p0803-incunabula:sbTitle
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:description
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:sideband_comment
    ] .

p0803-incunabula:sbTitle a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:Sideband;
  rdfs:label "Title";
  rdfs:subPropertyOf knora-api:hasValue, <http://purl.org/dc/terms/title> .

p0803-incunabula:description a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType knora-api:Resource;
  rdfs:comment "Description";
  rdfs:label "Description";
  rdfs:subPropertyOf knora-api:hasValue, <http://purl.org/dc/terms/description> .

p0803-incunabula:sideband_comment a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:Sideband;
  rdfs:comment "Unstrukturierte Bemerkungen zu einem Objekt";
  rdfs:label "Kommentar (Richtext)";
  rdfs:subPropertyOf knora-api:hasComment .

p0803-incunabula:book a owl:Class;
  knora-api:resourceIcon "book.gif";
  rdfs:comment "Diese Resource-Klasse beschreibt ein Buch";
  rdfs:label "Book";
  rdfs:subClassOf knora-api:Resource, [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasIncomingLink
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasStandoffLinkTo
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty knora-api:lastModificationDate
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty rdfs:label
    ], [ a owl:Restriction;
      owl:minCardinality 1;
      owl:onProperty p0803-incunabula:title
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:description
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:hasAuthor
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:publisher
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:publoc
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:citation
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:pubdate
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:location
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:url
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:physical_desc
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:note
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:book_comment
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty knora-api:creationDate
    ] .

p0803-incunabula:title a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Titel";
  rdfs:label "Title";
  rdfs:subPropertyOf knora-api:hasValue, <http://purl.org/dc/terms/title> .

p0803-incunabula:hasAuthor a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Erzeuger/Autor";
  rdfs:label "Creator";
  rdfs:subPropertyOf knora-api:hasValue, <http://purl.org/dc/terms/creator> .

p0803-incunabula:publisher a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Publishing is the process of production and dissemination of literature or information – the activity of making information available for public view. In some cases authors may be their own publishers, meaning: originators and developers of content also provide media to deliver and display the content. (Wikipedia http://en.wikipedia.org/wiki/Publisher)";
  rdfs:label "Publisher";
  rdfs:subPropertyOf knora-api:hasValue, <http://purl.org/dc/terms/publisher> .

p0803-incunabula:publoc a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Ort der Herausgabe";
  rdfs:label "Publication location";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:citation a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType knora-api:Resource;
  rdfs:comment "Stellt einen Verweis dar.";
  rdfs:label "Citation/reference";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:pubdate a owl:DatatypeProperty;
  knora-api:objectType knora-api:Date;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Datum der Herausgabe";
  rdfs:label "Datum der Herausgabe";
  rdfs:subPropertyOf knora-api:hasValue, <http://purl.org/dc/terms/date> .

p0803-incunabula:location a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Der Ort wo sich das physische Original befindet";
  rdfs:label "Location";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:url a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Uniform Resource Identifier";
  rdfs:label "URI";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:physical_desc a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Generelle physische Beschreibung des Objektes wie Material, Grösse etc.";
  rdfs:label "Phyiscal description";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:note a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "A note concerning the object";
  rdfs:label "Note";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:book_comment a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:book;
  rdfs:comment "Unstrukturierte Bemerkungen zu einem Objekt";
  rdfs:label "Comment";
  rdfs:subPropertyOf knora-api:hasComment .

p0803-incunabula:hasLeftSideband a owl:ObjectProperty;
  knora-api:objectType p0803-incunabula:Sideband;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "Verweis auf einen Randleistentyp";
  rdfs:label "Randleistentyp links";
  rdfs:subPropertyOf knora-api:hasLinkTo .

p0803-incunabula:page a owl:Class;
  knora-api:resourceIcon "page.gif";
  rdfs:comment "A page is a part of a book";
  rdfs:label "Page";
  rdfs:subClassOf knora-api:StillImageRepresentation, [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty knora-api:creationDate
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasIncomingLink
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasStandoffLinkTo
    ], [ a owl:Restriction;
      owl:minCardinality 1;
      owl:onProperty knora-api:hasStillImageFile
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty knora-api:lastModificationDate
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty rdfs:label
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:pagenum
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:description
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty p0803-incunabula:partOf
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:seqnum
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:citation
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:page_comment
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty p0803-incunabula:origname
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:hasLeftSideband
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:hasRightSideband
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty p0803-incunabula:transcription
    ] .

p0803-incunabula:hasRightSideband a owl:ObjectProperty;
  knora-api:objectType p0803-incunabula:Sideband;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "Verweis auf einen Randleistentyp";
  rdfs:label "Randleistentyp rechts";
  rdfs:subPropertyOf knora-api:hasLinkTo .

p0803-incunabula:misc a owl:Class;
  rdfs:comment "A fake resource class that only has optional properties";
  rdfs:label "Sonstiges";
  rdfs:subClassOf knora-api:Resource, [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty knora-api:creationDate
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasIncomingLink
    ], [ a owl:Restriction;
      owl:minCardinality 0;
      owl:onProperty knora-api:hasStandoffLinkTo
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty knora-api:lastModificationDate
    ], [ a owl:Restriction;
      owl:cardinality 1;
      owl:onProperty rdfs:label
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:miscHasColor
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:miscHasGeometry
    ], [ a owl:Restriction;
      owl:maxCardinality 1;
      owl:onProperty p0803-incunabula:miscHasBook
    ] .

p0803-incunabula:miscHasColor a owl:DatatypeProperty;
  knora-api:objectType knora-api:Color;
  knora-api:subjectType p0803-incunabula:misc;
  rdfs:label "Farbe";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:miscHasGeometry a owl:DatatypeProperty;
  knora-api:objectType knora-api:Geom;
  knora-api:subjectType p0803-incunabula:misc;
  rdfs:label "Geometrie";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:miscHasBook a owl:ObjectProperty;
  knora-api:objectType p0803-incunabula:book;
  knora-api:subjectType p0803-incunabula:misc;
  rdfs:label "Verbindung mit einem Buch";
  rdfs:subPropertyOf knora-api:hasLinkTo .

p0803-incunabula:origname a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "Der originale Dateiname";
  rdfs:label "Original filename";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:pagenum a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "A distinct identification of a book page";
  rdfs:label "Page identifier";
  rdfs:subPropertyOf knora-api:hasValue .

p0803-incunabula:partOf a owl:ObjectProperty;
  knora-api:objectType p0803-incunabula:book;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "Diese Property bezeichnet eine Verbindung zu einer anderen Resource, in dem ausgesagt wird, dass die vorliegende Resource ein integraler Teil der anderen Resource ist. Zum Beispiel ist eine Buchseite ein integraler Bestandteil genau eines Buches.";
  rdfs:label "is a part of";
  rdfs:subPropertyOf knora-api:isPartOf .

p0803-incunabula:seqnum a owl:DatatypeProperty;
  knora-api:objectType xsd:integer;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "This property stands for the position within a set of rdered items (resoucres)";
  rdfs:label "Sequence number";
  rdfs:subPropertyOf knora-api:seqnum .

p0803-incunabula:page_comment a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "Unstrukturierte Bemerkungen zu einem Objekt";
  rdfs:label "Comment";
  rdfs:subPropertyOf knora-api:hasComment .

p0803-incunabula:transcription a owl:DatatypeProperty;
  knora-api:objectType xsd:string;
  knora-api:subjectType p0803-incunabula:page;
  rdfs:comment "Transkription";
  rdfs:label "Transkription";
  rdfs:subPropertyOf knora-api:hasValue .
