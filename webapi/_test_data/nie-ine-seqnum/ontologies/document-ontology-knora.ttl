<http://www.knora.org/ontology/005C/document>
	a owl:Ontology;
	dct:title "An ontology about document"@en;
	dct:description """Formal description of document, general subclasses, related classes and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en;

	knora-base:attachedToProject <http://rdfh.ch/projects/005C>.


# EMBRYONIC: declared 'middle-out', on the go as concepts are needed, not intended to contain upfront all elements


#
# CLASSES
#

document:Document
	a owl:Class;
	rdfs:label "document"@en, "Dokument"@de;
	rdfs:comment """Identifiable, immaterial, contained information, e.g. a text, a graphic, an image, an audiogram, a videogram."""@en;
	rdfs:subClassOf concept:Information. # , cidoc:E31_Document: proposition about reality

document:Element
	a owl:Class;
	rdfs:label "document element"@en, "Dokumentelement"@de;
	rdfs:comment """Information as part of a document."""@en;
	rdfs:subClassOf concept:Information, [
		a owl:Restriction; owl:onProperty document:isElementOfDocument; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:isElementOfDocumentValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Structure
	a owl:Class;
	rdfs:label "document structure"@en, "Dokumentstruktur"@de;
	rdfs:comment """Structural element of a document."""@en;
	rdfs:subClassOf document:Element, [
		a owl:Restriction; owl:onProperty infocar:hasHyperlink; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ #**
		a owl:Restriction; owl:onProperty infocar:hasHyperlinkValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:ContentStructure
	a owl:Class;
	rdfs:label "document content structure"@en, "Dokumentinhaltstruktur"@de;
	rdfs:comment """Structural content element of a document."""@en;
	rdfs:subClassOf document:Structure, [
		a owl:Restriction; owl:onProperty document:hasFootnoteReferenceIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasFootnoteReferenceIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasFootnoteReferenceNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasFootnoteReferenceNumberValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasEndnoteReferenceIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasEndnoteReferenceIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasEndnoteReferenceNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasEndnoteReferenceNumberValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Reference
	a owl:Class;
	rdfs:label "document reference"@en, "Dokumentreferenz"@de;
	rdfs:comment """Reference as a structural element in a document."""@en;
	rdfs:subClassOf document:Structure.

document:FootnoteReferenceIdentifier
	a owl:Class;
	rdfs:label "footnote reference identifier"@en, "Fussnotereferenzidentifikator"@de;
	rdfs:comment """Identifier of a footnote reference."""@en;
	rdfs:subClassOf concept:Identifier, document:Reference, [
		a owl:Restriction; owl:onProperty document:hasFootnoteIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasFootnoteIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:FootnoteReferenceNumber
	a owl:Class;
	rdfs:label "footnote reference number"@en, "Fussnotereferenznummer"@de;
	rdfs:comment """Number of a footnote reference."""@en;
	rdfs:subClassOf document:FootnoteReferenceIdentifier, [
		a owl:Restriction; owl:onProperty document:hasFootnoteNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasFootnoteNumberValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:EndnoteReferenceIdentifier
	a owl:Class;
	rdfs:label "endnote reference identifier"@en, "Endnotereferenzidentifikator"@de;
	rdfs:comment """Identifier of an endnote reference."""@en;
	rdfs:subClassOf concept:Identifier, document:Reference, [
		a owl:Restriction; owl:onProperty document:hasEndnoteIdentifier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasEndnoteIdentifierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:EndnoteReferenceNumber
	a owl:Class;
	rdfs:label "endnote reference number"@en, "Endnotereferenznummer"@de;
	rdfs:comment """Number of an endnote reference."""@en;
	rdfs:subClassOf document:EndnoteReferenceIdentifier, [ # math:NaturalNumber
		a owl:Restriction; owl:onProperty document:hasEndnoteNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:hasEndnoteNumberValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Layout
	a owl:Class;
	rdfs:label "layout"@en, "Aufmachung"@de;
	rdfs:comment """Way in which the content of a page is set out."""@en;
	rdfs:subClassOf knora-base:Resource. #* document:Element, concept:Layout

document:Column # make a text: SC? - ReadabilityStructure
	a owl:Class;
	rdfs:label "column"@en, "Spalte"@de;
	rdfs:comment """Document structure displaying information, e.g. text lines and graphs, as a column as part of a page."""@en;
	rdfs:subClassOf document:ContentStructure, [
#		a owl:Restriction; owl:onProperty document:isColumnOfPage; owl:allValuesFrom document:Page], [
		a owl:Restriction; owl:onProperty document:isColumnOfPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:isColumnOfPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Page # text, visuals, page # etc. # make a text: SC?
	a owl:Class;
	rdfs:label "document page"@en, "Dokumentseite"@de;
	rdfs:comment """Document structure displaying information e.g. text lines and graphs, as a page as part of a document."""@en;
	skos:note """It is a column filling a complete information carrier page."""@en;
	rdfs:subClassOf document:Column, [
#		a owl:Restriction; owl:onProperty document:isPageOfDocument; owl:allValuesFrom document:Document], [
		a owl:Restriction; owl:onProperty document:isPageOfDocument; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:isPageOfDocumentValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:TitlePage
	a owl:Class;
	rdfs:label "title page"@en, "Titelblatt"@de;
	rdfs:comment """Document page contianing a title."""@en;
	rdfs:subClassOf document:Page. # , [
#		a owl:Restriction; owl:onProperty document:isPageOfDocument; owl:allValuesFrom document:Document], [
#		a owl:Restriction; owl:onProperty document:isPageOfDocument; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty document:isPageOfDocumentValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Marginal
	a owl:Class;
	rdfs:label "marginal"@en, "Marginale"@de; # in GUI can be "marginalia"@en, "Marginalien"@de
	rdfs:comment """Information besides a text in the margin of a carrier, e.g. text, doodles, illuminations."""@en;
	rdfs:subClassOf document:ContentStructure.

document:Table #* tableHasTitle, tableHasField, tableHasRow, tableHasColumn
	a owl:Class;
	rdfs:label "table"@en, "Tabelle"@de;
	rdfs:comment """Two-dimensional structure composed of one or more fields containing information and being part of a vertical column and a horizontal row, both with own identifier."""@en;
	rdfs:subClassOf document:ContentStructure, [
		a owl:Restriction; owl:onProperty document:isTableOfColumn; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:isTableOfColumnValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Expression
	a owl:Class;
	rdfs:label "document expression"@en, "Document-Expression"@de;
	rdfs:comment """Document realisation of an individual work conform FRBROO, e.g. a text, story, poem, play, musical, song, joke, choreographic notation, movement pattern, sound pattern, image, multimedia object, or any combination of them, having an objectively recognisable structure."""@en;
	rdfs:subClassOf document:Document, frbroo:F22_Self-Contained_Expression, [
		a owl:Restriction; owl:onProperty document:expressionHasTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:isExpressionOfCycle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty document:isExpressionOfCycleValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublishingState; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublishingStateValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublishing; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublishingValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasElectronicPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasElectronicPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasWebPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasWebPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPublicationNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasFirstPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasFirstPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [		
		a owl:Restriction; owl:onProperty publish:hasLastPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasLastPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasLastAuthorizedPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasLastAuthorizedPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasUnauthorizedPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasUnauthorizedPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasUnauthorizedRepublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasUnauthorizedRepublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPolyAuthorPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasPolyAuthorPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasNachlassPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasNachlassPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty publish:hasNachlassPublicationDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger].

document:Cycle
	a owl:Class;
	rdfs:label "expression cycle"@en, "Expression-Zyklus"@de;
	rdfs:comment """Set of expressions belonging together, usually created around a certain theme, and intended to be experienced in a certain sequence."""@en; #*
	rdfs:subClassOf concept:Set.

