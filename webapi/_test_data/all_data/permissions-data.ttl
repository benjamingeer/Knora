##########################################################
#
# Knora System Permissions
#
##########################################################


### Default Object Access Permissions on knora-base:LinkObj resource class
<http://data.knora.org/permissions/001-1>

                      rdf:type knora-base:DefaultObjectAccessPermissionOnResourceClass ;

                      knora-base:forProject knora-base:SystemProject ;

                      knora-base:forResourceClass knora-base:LinkObj ;

                      knora-base:hasPermissions "DM knora-base:ProjectMember|DV knora-base:KnownUser, knora-base:UnknownUser"^^xsd:string .



### Default Object Access Permissions on knora-base:Region resource class
<http://data.knora.org/permissions/001-2>

                      rdf:type knora-base:DefaultObjectAccessPermissionOnResourceClass ;

                      knora-base:forProject knora-base:SystemProject ;

                      knora-base:forResourceClass knora-base:Region ;

                      knora-base:hasPermissions "DM knora-base:ProjectMember|DV knora-base:KnownUser, knora-base:UnknownUser"^^xsd:string .



### Default Object Access Permissions on knora-base:hasStillImageFileValue property
<http://data.knora.org/permissions/001-3>

                      rdf:type knora-base:DefaultObjectAccessPermissionOnProperty ;

                      knora-base:forProject knora-base:SystemProject ;

                      knora-base:forProperty knora-base:hasStillImageFileValue ;

                      knora-base:hasPermissions "DM knora-base:Creator, knora-base:ProjectMember|DV knora-base:KnownUser|DRV knora-base:UnknownUser"^^xsd:string .



