@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix foaf:<http://xmlns.com/foaf/0.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix event: <http://purl.org/NET/c4dm/event.owl#> .
@prefix knora-base: <http://www.knora.org/ontology/knora-base#> .
@prefix salsah-gui: <http://www.knora.org/ontology/salsah-gui#> .
@prefix beol: <http://www.knora.org/ontology/0801/beol#> .
@prefix biblio: <http://www.knora.org/ontology/0801/biblio#> .
@base <http://www.knora.org/ontology/0801/newton#> .

@prefix : <http://www.knora.org/ontology/0801/newton#> .

<http://www.knora.org/ontology/0801/newton> rdf:type owl:Ontology ;
    rdfs:label "The Newton-Project ontology" ;
    knora-base:attachedToProject <http://rdfh.ch/projects/yTerZGyxjZVqFMNNKXCDPF> .

##########################################################
#
# PROPERTIES
#
##########################################################


### ###########################################
### newton:hasAbbreviatedTitle

:hasAbbreviatedTitle rdf:type owl:ObjectProperty ;


                rdfs:label "Abkürzung des Titels"@de ,
                           "Abbreviation of Title"@en ;

                rdfs:comment """Represents the abbreviated title of the letter."""@en ;

                knora-base:objectClassConstraint knora-base:TextValue ;
                knora-base:subjectClassConstraint :letter ;

                salsah-gui:guiElement salsah-gui:SimpleText ;

                rdfs:subPropertyOf knora-base:hasValue .

### ###########################################
### newton:fileName

:fileName rdf:type owl:ObjectProperty ;


                rdfs:label "Datei-Name"@de ,
                           "File Name of letter"@en ;

                rdfs:comment """Represents the filename of the letter."""@en ;

                knora-base:objectClassConstraint knora-base:TextValue ;
                knora-base:subjectClassConstraint :letter ;

                salsah-gui:guiElement salsah-gui:SimpleText ;

                rdfs:subPropertyOf knora-base:hasValue .

### ###########################################
### newton:hasFacsimiles

:hasFacsimiles rdf:type owl:ObjectProperty ;

                   knora-base:objectClassConstraint knora-base:UriValue ;

                   knora-base:subjectClassConstraint :letter ;

                   rdfs:subPropertyOf knora-base:hasValue ;

                   rdfs:label "Die Faksimilen von dem Brief"@de ,
                              "Facsimiles of letter"@en ;

                   rdfs:comment """Die Faksimilen von deisem Brief ist vorhanden in"""@de ,
                              """The Facsimiles of this letter are available in"""@en .

### ###########################################
### newton:hasAvailability

:hasAvailability rdf:type owl:ObjectProperty ;

              rdfs:subPropertyOf knora-base:hasLinkTo ;

              rdfs:label "Gültigkeit-Beschreibung"@de ,
                         "Publication Availibility"@en ;

              rdfs:comment "A resource representing licensed availability of a published letter"@en ;

              knora-base:subjectClassConstraint :publicationStmt ;

              knora-base:objectClassConstraint :license ;

              salsah-gui:guiElement salsah-gui:Searchbox .


### ###########################################
### newton:hasAvailabilityValue

:hasAvailabilityValue rdf:type owl:ObjectProperty ;

                   knora-base:objectClassConstraint knora-base:LinkValue ;

                   rdfs:subPropertyOf knora-base:hasLinkToValue ;


                   rdfs:label "Gültigkeit-Beschreibung"@de ,
                              "Publication Availibility"@en ;

                   rdfs:comment "A resource representing licensed availability of a published letter"@en .


### ###########################################
### newton:hasAuthority

:hasAuthority rdf:type owl:ObjectProperty ;


                rdfs:label "Authorität"@de ,
                           "Authority"@en ;

                rdfs:comment """Represents the authority owner of data."""@en ;

                knora-base:objectClassConstraint knora-base:TextValue ;
                knora-base:subjectClassConstraint :publicationStmt ;

                salsah-gui:guiElement salsah-gui:SimpleText ;

                rdfs:subPropertyOf knora-base:hasValue .



### ###########################################
### newton:licenseNumber

:licenseNumber rdf:type owl:ObjectProperty ;


                rdfs:label "Lizenznummer"@de ,
                           "license Number"@en ;

                rdfs:comment """Represents the license number for publication of a letter."""@en ;

                knora-base:objectClassConstraint knora-base:TextValue ;
                knora-base:subjectClassConstraint :license ;

                salsah-gui:guiElement salsah-gui:SimpleText ;

                rdfs:subPropertyOf knora-base:hasValue .

### ###########################################
### newton:licenseStatus

:licenseStatus rdf:type owl:ObjectProperty ;


             rdfs:label "Lizenzstatus"@de ,
                        "license Status"@en ;

             rdfs:comment """Represents the status of a license."""@en ;

             knora-base:objectClassConstraint knora-base:TextValue ;
             knora-base:subjectClassConstraint :license ;

             salsah-gui:guiElement salsah-gui:SimpleText ;

             rdfs:subPropertyOf knora-base:hasValue .

### ###########################################
### newton:hasChanges

:hasChanges rdf:type owl:ObjectProperty ;

              rdfs:subPropertyOf knora-base:hasLinkTo ;

              rdfs:label "Änderungen-Beschreibung"@de ,
                         "Changes Description"@en ;

              rdfs:comment "A resource representing the changes made in every revision"@en ;

              knora-base:subjectClassConstraint :revisionDesc ;

              knora-base:objectClassConstraint :change ;

              salsah-gui:guiElement salsah-gui:Searchbox .


### ###########################################
### newton:hasChangesValue

:hasChangesValue rdf:type owl:ObjectProperty ;

                   knora-base:objectClassConstraint knora-base:LinkValue ;

                   rdfs:subPropertyOf knora-base:hasLinkToValue ;


                   rdfs:label "Änderungen-Beschreibung"@de ,
                             "Changes Description"@en ;

                   rdfs:comment "A resource representing the changes made in every revision"@en .

### ###########################################
### newton:editor

:editor rdf:type owl:ObjectProperty ;

              rdfs:subPropertyOf knora-base:hasLinkTo ;

              rdfs:label "Editor"@de ,
                         "Editor"@en ;

              rdfs:comment "A resource representing the editor who made the change on data"@en ;

              knora-base:subjectClassConstraint :change ;

              knora-base:objectClassConstraint beol:person ;

              salsah-gui:guiElement salsah-gui:Searchbox .


### ###########################################
### newton:editorValue

:editorValue rdf:type owl:ObjectProperty ;

                   knora-base:objectClassConstraint knora-base:LinkValue ;

                   rdfs:subPropertyOf knora-base:hasLinkToValue ;


                   rdfs:label "Editor"@de ,
                              "Editor"@en ;

                   rdfs:comment "A resource representing the editor who made the change on data"@en .



### ###########################################
### newton:revisionDate
:revisionDate rdf:type owl:ObjectProperty ;

                    rdfs:label "Datum der Änderung "@de ,
                               "Revision Date "@en ;


                    rdfs:comment """Represents a Revision Date."""@en ;

                    knora-base:subjectClassConstraint knora-base:Resource ;

                    knora-base:objectClassConstraint knora-base:DateValue  ;

                    salsah-gui:guiElement salsah-gui:Date ;

                    rdfs:subPropertyOf knora-base:hasValue, dcterms:issued .


##########################################################
#
# RESOURCES
#
##########################################################

### ###########################################
### newton:letter

:letter rdf:type owl:Class ;

        rdfs:subClassOf beol:letter ,
                       [
                          rdf:type owl:Restriction ;
                          owl:onProperty :hasAbbreviatedTitle ;
                          owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ,
                       [
                          rdf:type owl:Restriction ;
                          owl:onProperty :fileName ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger
                       ] ,
                       [
                          rdf:type owl:Restriction ;
                          owl:onProperty :hasFacsimiles ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "2"^^xsd:nonNegativeInteger
                       ] ;

        knora-base:resourceIcon "letter.png" ;

        rdfs:label "Brief"@de ,
                   "Lettre"@fr ,
                   "Letter"@en ;

        rdfs:comment "A resource representing a letter from The Newton Project"@en .


### ###########################################
### newton:publicationStmt
:publicationStmt rdf:type owl:Class ;

        rdfs:subClassOf knora-base:Resource ,
                       [
                            rdf:type owl:Restriction ;
                            owl:onProperty :hasAuthority;
                            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                            salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ,
                       [
                            rdf:type owl:Restriction ;
                            owl:onProperty :hasAvailability ;
                            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                            salsah-gui:guiOrder "6"^^xsd:nonNegativeInteger
                       ] ,
                       [
                            rdf:type owl:Restriction ;
                            owl:onProperty :hasAvailabilityValue ;
                            owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                            salsah-gui:guiOrder "6"^^xsd:nonNegativeInteger
                       ] ,
                       [
                           rdf:type owl:Restriction ;
                           owl:onProperty biblio:publicationHasDate ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                           salsah-gui:guiOrder "4"^^xsd:nonNegativeInteger
                       ] ,
                       [
                           rdf:type owl:Restriction ;
                           owl:onProperty biblio:publicationHasLocation ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                           salsah-gui:guiOrder "5"^^xsd:nonNegativeInteger
                       ] ,
                       [
                           rdf:type owl:Restriction ;
                           owl:onProperty biblio:publicationHasPublisher ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                           salsah-gui:guiOrder "8"^^xsd:nonNegativeInteger
                       ]  ,
                       [
                           rdf:type owl:Restriction ;
                           owl:onProperty biblio:publicationHasPublisherValue ;
                           owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                           salsah-gui:guiOrder "8"^^xsd:nonNegativeInteger
                       ] ;

        rdfs:label "Publikation-Beschreibung"@de ,
                   "Publication Description"@en ;

        rdfs:comment "A resource representing a statement about publication of letter"@en .


### ###########################################
### newton:license
:license rdf:type owl:Class ;

        rdfs:subClassOf knora-base:Resource ,
                       [
                            rdf:type owl:Restriction ;
                            owl:onProperty :licenseNumber;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                            salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ,
                       [
                           rdf:type owl:Restriction ;
                           owl:onProperty :licenseStatus;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                           salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ,
                       [
                            rdf:type owl:Restriction ;
                            owl:onProperty beol:hasText;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                            salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ;

        rdfs:label "Lizenz-Beschreibung"@de ,
                   "License"@en ;

        rdfs:comment "A resource representing publication license"@en .


### ###########################################
### newton:revisionDesc
:revisionDesc rdf:type owl:Class ;

        rdfs:subClassOf knora-base:Resource ,
                       [
                          rdf:type owl:Restriction ;
                          owl:onProperty :hasChanges ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ,
                       [
                          rdf:type owl:Restriction ;
                          owl:onProperty :hasChangesValue ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                       ] ;

        rdfs:label "Überarbeitung-Beschreibung"@de ,
                   "Revision Description"@en ;

        rdfs:comment "A resource representing the description of revisions of data."@en .

### ###########################################
### newton:change
:change rdf:type owl:Class ;

        rdfs:subClassOf knora-base:Resource ,
                      [
                          rdf:type owl:Restriction ;
                          owl:onProperty :editor ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                          rdf:type owl:Restriction ;
                          owl:onProperty :editorValue ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                          rdf:type owl:Restriction ;
                          owl:onProperty :revisionDate ;
                          owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                          salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger
                      ] ;

        rdfs:label "Änderung"@de ,
                   "Change"@en ;

        rdfs:comment "A resource representing a change made on data."@en .
