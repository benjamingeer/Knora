<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../src/main/resources/mappingXMLToStandoff.xsd">
  <mappingElement>
    <tag><name>text</name><class>noClass</class><namespace>noNamespace</namespace></tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffRootTag</classIri>
      <attributes>
        <attribute><attributeName>documentType</attributeName><namespace>noNamespace</namespace><propertyIri>http://www.knora.org/ontology/standoff#standoffRootTagHasDocumentType</propertyIri></attribute>
      </attributes>
    </standoffClass>
  </mappingElement>

  <mappingElement>
    <tag>
      <name>p</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffParagraphTag</classIri>
    </standoffClass>
  </mappingElement>

  <mappingElement>
    <tag>
      <name>em</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffItalicTag</classIri>
    </standoffClass>
  </mappingElement>

  <mappingElement>
    <tag>
      <name>strong</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffBoldTag</classIri>
    </standoffClass>
  </mappingElement>

  <mappingElement>
    <tag>
      <name>u</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace></tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffUnderlineTag</classIri>
    </standoffClass>
  </mappingElement>
  
  <mappingElement>
    <tag>
      <name>sub</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffSubscriptTag</classIri>
    </standoffClass>
  </mappingElement>
  
  <mappingElement>
    <tag>
      <name>sup</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffSuperscriptTag</classIri>
    </standoffClass>
  </mappingElement>

  <mappingElement>
    <tag>
      <name>strike</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/standoff#StandoffStrikethroughTag</classIri>
    </standoffClass>
  </mappingElement>

  <mappingElement>
    <tag>
      <name>a</name>
      <class>noClass</class>
      <namespace>noNamespace</namespace>
    </tag>
    <standoffClass>
      <classIri>http://www.knora.org/ontology/knora-base#StandoffUriTag</classIri>
      <datatype>
        <type>http://www.knora.org/ontology/knora-base#StandoffUriTag</type>
        <attributeName>href</attributeName>
      </datatype>
    </standoffClass>
  </mappingElement>

<mappingElement>
  <tag>
    <name>a</name>
    <class>salsah-link</class>
    <namespace>noNamespace</namespace>
  </tag>
  <standoffClass>
    <classIri>http://www.knora.org/ontology/knora-base#StandoffLinkTag</classIri>
    <datatype>
      <type>http://www.knora.org/ontology/knora-base#StandoffLinkTag</type>
      <attributeName>href</attributeName>
    </datatype>
  </standoffClass>
</mappingElement>
</mapping>
